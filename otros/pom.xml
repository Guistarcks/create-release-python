<?xml version='1.0' encoding='utf-8'?>
<ns0:project xmlns:ns0="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
    <ns0:modelVersion>4.0.0</ns0:modelVersion>

    <ns0:groupId>org.springframework</ns0:groupId>
    <ns0:artifactId>gs-maven</ns0:artifactId>
    <ns0:packaging>jar</ns0:packaging>
    <ns0:version>1.5.0-SNAPSHOT</ns0:version>

    <ns0:properties>
        <ns0:maven.compiler.source>1.8</ns0:maven.compiler.source>
        <ns0:maven.compiler.target>1.8</ns0:maven.compiler.target>
    </ns0:properties>

    <ns0:build>
        <ns0:plugins>
            <ns0:plugin>
                <ns0:groupId>org.apache.maven.plugins</ns0:groupId>
                <ns0:artifactId>maven-shade-plugin</ns0:artifactId>
                <ns0:version>3.2.4</ns0:version>
                <ns0:executions>
                    <ns0:execution>
                        <ns0:phase>package</ns0:phase>
                        <ns0:goals>
                            <ns0:goal>shade</ns0:goal>
                        </ns0:goals>
                        <ns0:configuration>
                            <ns0:transformers>
                                <ns0:transformer implementation="org.apache.maven.plugins.shade.resource.ManifestResourceTransformer">
                                    <ns0:mainClass>hello.HelloWorld</ns0:mainClass>
                                </ns0:transformer>
                            </ns0:transformers>
                        </ns0:configuration>
                    </ns0:execution>
                </ns0:executions>
            </ns0:plugin>
        </ns0:plugins>
    </ns0:build>
</ns0:project>